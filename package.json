{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "node .output/server/index.mjs",
    "build": "nuxt build --prerender",
    "clean": "rimraf .nuxt dist .cache .output node_modules",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint:js": "eslint --ext \".ts,.vue\" --ignore-path .gitignore .",
    "lint:prettier": "prettier --check .",
    "lint": "yarn lint:js && yarn lint:prettier",
    "lintfix": "prettier --write --list-different . && pnpm lint:js --fix",
    "lhci:mobile": "lhci autorun",
    "unlighthouse": "dotenv -- cross-var pnpm dlx unlighthouse --site %NUXT_PUBLIC_UNLIGHTHOUSE%"
  },
  "devDependencies": {
    "@lhci/cli": "^0.12.0",
    "@nuxt/devtools": "latest",
    "@nuxt/image": "^1.1.0",
    "@nuxtjs/color-mode": "^3.3.2",
    "@nuxtjs/eslint-config-typescript": "^12.1.0",
    "@nuxtjs/eslint-module": "^4.1.0",
    "@nuxtjs/google-fonts": "^3.1.0",
    "@nuxtjs/partytown": "^1.3.0",
    "@nuxtjs/tailwindcss": "^6.10.1",
    "@rushstack/eslint-patch": "^1.6.0",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.10",
    "@typescript-eslint/parser": "^6.13.1",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "cross-var": "^1.1.0",
    "daisyui": "^4.4.14",
    "dotenv": "^16.3.1",
    "dotenv-cli": "^7.3.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-universe": "^12.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "flowbite": "^2.2.0",
    "nuxt": "^3.8.2",
    "nuxt-gtag": "^1.1.1",
    "nuxt-icon": "^0.6.6",
    "prettier": "^3.1.0",
    "rimraf": "^5.0.5",
    "sass": "^1.69.5",
    "typescript": "^5.3.2",
    "vue": "^3.3.8",
    "vue-eslint-parser": "^9.3.2",
    "vue-router": "^4.2.5"
  },
  "dependencies": {
    "@headlessui/vue": "^1.7.16",
    "@nuxt/content": "^2.9.0",
    "@vercel/analytics": "^1.1.1",
    "uuid": "^9.0.1"
  }
}
